{
  "name": "clock-time",
  "version": "1.2.0",
  "description": "Formats milliseconds or minutes and seconds as clock time",
  "author": "Trevor Blades <tdblades@gmail.com>",
  "repository": "github:trevorblades/clock-time",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "clock",
    "time",
    "minutes",
    "seconds",
    "milliseconds",
    "format",
    "formatter"
  ],
  "scripts": {
    "pretest": "xo",
    "test": "ava",
    "prepare": "babel src --out-dir lib --ignore=**/test.js"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ]
  },
  "xo": {
    "space": true,
    "prettier": true
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "dependencies": {
    "left-pad": "^1.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@babel/register": "^7.0.0-beta.42",
    "ava": "^1.0.0-beta.3",
    "xo": "^0.20.3"
  }
}
